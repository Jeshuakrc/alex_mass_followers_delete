{
   "action": {
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "script.js" ],
      "matches": [ "https://*.twitter.com/*/followers" ]
   } ],
   "description": "A simple (configurable) way to mass unfollow your Twitter followings",
   "icons": {
      "128": "images/128.png",
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Twitter mass follower delete",
   "options_page": "options.html",
   "permissions": [ "storage", "declarativeContent", "scripting", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.5",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "shared.js" ]
   } ]
}
